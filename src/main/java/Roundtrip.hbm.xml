<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.thumbsup.coolname.entity.RoundTrip" table="RoundTrip">
   	
   	<meta attribute="class-description">
   		"Contains the details on the location entity class."
   	</meta>
   	<id name="RoundTripID" type="int" column="RoundTripID">
   		<generator class="native"></generator>
   	</id>

   	<property name="StartRideEntryID" column="StartRideEntryID" type="int"/>
   	<property name="EndRideEntryID" column="EndRideEntryID" type="int"/>

   </class>
</hibernate-mapping>