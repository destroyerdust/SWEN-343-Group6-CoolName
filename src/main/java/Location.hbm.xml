<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.thumbsup.coolname.entity.Location" table="Location">
   	
   	<meta attribute="class-description">
   		"Contains the details on the location entity class."
   	</meta>
   	<id name="idLocation" type="int" column="idLocation">
   		<generator class="native"></generator>
   	</id>
   	<property name="area" column="Area" type="string"/>
   	<property name="groupID" column="GroupID" type="int"/>
   </class>
</hibernate-mapping>