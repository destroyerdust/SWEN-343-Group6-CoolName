<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.thumbsup.coolname.entity.Signup" table="Signup">
      <meta attribute="class-description">
         This class contains the signup detail. 
      </meta>
      <id name="rideOnID" type="int" column="RideOnID">
         <generator class="native"/>
      </id>
      <property name="rideEntryID" column="RideEntryID" type="int"/>
      <property name="timestamp" column="Timestamp" type="timestamp"/>
      <property name="userID" column="UserID" type="int"/>   
   </class>
</hibernate-mapping>